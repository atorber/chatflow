{
  "name": "file-name-linter",
  "version": "1.1.0",
  "description": "validates if the file names in one or more directories match a convention",
  "main": "index.js",
  "bin": "./lib/cli.js",
  "scripts": {
    "pretest": "standard",
    "test": "nyc mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/winniehell/file-name-linter.git"
  },
  "keywords": [
    "lint",
    "files",
    "naming",
    "conventions"
  ],
  "author": "winniehell",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/winniehell/file-name-linter/issues"
  },
  "homepage": "https://gitlab.com/winniehell/file-name-linter#README",
  "devDependencies": {
    "mocha": "^3.2.0",
    "must": "^0.13.4",
    "nyc": "^10.1.2",
    "standard": "^8.6.0"
  },
  "nyc": {
    "all": true,
    "exclude": [
      "coverage/",
      "test/"
    ],
    "reporter": [
      "html",
      "text-summary"
    ],
    "check-coverage": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100
  }
}
